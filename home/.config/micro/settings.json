{
	"autoindent": true,
	"autosave": 0,
	"autosu": false,
    "backup": false,
    "backupdir": "~/.config/micro/backups",
    "basename": true,
    "clipboard": "terminal",
    "colorcolumn": 0,
    "colorscheme": "simple",
    "cursorline": true,
    "detectlimit": 100,
    "diffgutter": false,
    "divchars": "|-",
    "divreverse": false,
    "encoding": "utf-8",
    "eofnewline": false,
    "fakecursor": false,
    "fastdirty": true,
    "fileformat": "unix",
    "filetype": "unknown",
    "fzf": true,
	"helpsplit": "hsplit",
	"hlsearch": true,
    "hltaberrors": false,
    "hltrailingws": true,
	"ignorecase": true,
	"incsearch": true,
	"indentchar": " ",
	"infobar": false,
	"keepautoindent": true,
	"keymenu": false,
	"matchbrace": true,
    "matchbraceleft": true,
    "matchbracestyle": "underline",
    "mkparents": true,
    "mouse": true,
    "multiopen": "vsplit",
    "pageoverlap": 3,
    "parsecursor": false,
    "paste": false,
    "permbackup": false,
    "pluginchannels": [ "https://raw.githubusercontent.com/micro-editor/plugin-channel/master/channel.json" ],
    "pluginrepos": [],
    "quoter": true,
    "readonly": false,
    "relativeruler": false,
    "reload": "disabled",
    "rmtrailingws": false,
    "ruler": true,
    "savecursor": true,
    "savehistory": true,
    "saveundo": true,
    "scrollbar": false,
    "scrollbarchar": "|",
    "scrollmargin": 3,
    "scrollspeed": 2,
    "smartpaste": true,
    "softwrap": true,
    "splitbottom": true,
    "splitright": true,
    "statusformatl": "$(filename)",
    "statusformatr": "",
    "statusline": false,
    "sucmd": "sudo",
    "syntax": true,
    "tabhighlight": false,
    "tabmovement": false,
    "tabreverse": true,
    "tabsize": 4,
    "tabstospace": false,
    "useprimary": true,
    "wordwrap": true,
    "xterm": false
}