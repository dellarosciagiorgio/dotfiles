{
    "autoindent": true,
    "autosave": 0,
    "backup": false,
    "backupdir": "~/.config/micro/backups",
    "basename": false,
    "clipboard": "internal",
    "colorcolumn": 0,
    "colorscheme": "simple",
    "cursorline": true,
    "detectlimit": 100,
    "diffgutter": false,
    "divchars": "|-",
    "divreverse": true,
    "encoding": "utf-8",
    "eofnewline": false,
    "fakecursor": false,
    "fastdirty": false,
    "fileformat": "unix",
    "filetype": "unknown",
    "hlsearch": true,
    "hltaberrors": false,
    "hltrailingws": false,
    "incsearch": true,
    "ignorecase": true,
    "indentchar": " ",
    "infobar": true,
    "keepautoindent": false,
    "keymenu": false,
    "matchbrace": true,
    "matchbraceleft": true,
    "matchbracestyle": "highlight",
    "mkparents": true,
    "mouse": true,
    "multiopen": "vsplit",
    "paste": true,
    "parsecursor": false,
    "permbackup": false,
    "pluginchannels": [
        "https://raw.githubusercontent.com/micro-editor/plugin-channel/master/channel.json"
    ],
    "pluginrepos": [],
    "readonly": false,
    "reload": "prompt",
    "rmtrailingws": true,
    "ruler": true,
    "relativeruler": false,
    "savecursor": false,
    "savehistory": false,
    "saveundo": false,
    "scrollbar": false,
    "scrollbarchar": "|",
    "scrollmargin": 3,
    "scrollspeed": 2,
    "smartpaste": true,
    "softwrap": true,
    "splitbottom": true,
    "splitright": true,
    "statusformatl": "$(filename) $(modified)($(line),$(col)) $(status.paste)| ft:$(opt:filetype) | $(opt:fileformat) | $(opt:encoding)",
    "statusformatr": "$(bind:ToggleKeyMenu): bindings, $(bind:ToggleHelp): help",
    "statusline": true,
    "sucmd": "sudo",
    "syntax": true,
    "tabmovement": false,
    "tabhighlight": false,
    "tabreverse": true,
    "tabsize": 4,
    "tabstospaces": false,
    "useprimary": true,
    "wordwrap": true,
    "xterm": false
}