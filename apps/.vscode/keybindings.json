[
    // view and manage
    {
        "key": "f1",
        "command": "workbench.action.showCommands"
    },
    {
        "key": "f2",
        "command": "-editor.action.refactor",
        "when": "editorHasRenameProvider && editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+o",
        "command": "workbench.action.files.openFile"
    },
    {
        "key": "ctrl+k ctrl+o",
        "command": "workbench.action.files.openFolder"
    },
    {
        "key":"cmd+escape",
        "command": "workbench.action.closeWindow"
    },
    {
        "key": "ctrl+n",
        "command": "explorer.newFile",
        "when": "filesExplorerFocus"
    },
    {
        "key": "ctrl+shift+l",
        "command": "expandLineSelection",
        "when": "textInputFocus"
    },
    {
        "key": "ctrl+shift+n",
        "command": "explorer.newFolder",
        "when": "filesExplorerFocus"
    },
    {
        "key": "ctrl+k ctrl+t",
        "command": "workbench.action.terminal.new"
    },
    {
        "key": "ctrl+k ctrl+k",
        "command": "workbench.action.selectTheme"
    },
    {
        "key": "ctrl+w",
        "command": "workbench.action.closeActiveEditor",
        "when": "editorFocus"
    },
    {
        "key": "ctrl+a",
        "command": "editor.action.selectAll",
        "when": "textInputFocus"
    },
    // movement
    {
        "key": "up",
        "command": "cursorUp",
        "when": "textInputFocus"
    },
    {
        "key": "right",
        "command": "cursorRight",
        "when": "textInputFocus"
    },
    {
        "key": "left",
        "command": "cursorLeft",
        "when": "textInputFocus"
    },
    {
        "key": "down",
        "command": "cursorDown",
        "when": "textInputFocus"
    },
    {
        "key": "ctrl+alt+up",
        "command": "editor.action.insertCursorAbove",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+alt+down",
        "command": "editor.action.insertCursorBelow",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+alt+q",
        "command": "workbench.action.previousEditor"
    },
    {
        "key": "ctrl+alt+w",
        "command": "workbench.action.nextEditor"
    },
    {
        "key": "cmd+alt+ctrl+left",
        "command": "cursorHomeSelect",
        "when": "textInputFocus"
    },
    {
        "key": "cmd+alt+ctrl+right",
        "command": "cursorEndSelect",
        "when": "textInputFocus"
    },
    {
        "key": "ctrl+right",
        "command": "cursorWordPartRight",
        "when": "textInputFocus && !editorReadOnly"
    },
    {
        "key": "ctrl+left",
        "command": "cursorWordPartLeft",
        "when": "textInputFocus && !editorReadOnly"
    },
    {
        "key": "ctrl+shift+left",
        "command": "cursorWordPartLeftSelect",
        "when": "textInputFocus && !editorReadOnly"
    },
    {
        "key": "ctrl+shift+right",
        "command": "cursorWordPartRightSelect",
        "when": "textInputFocus && !editorReadOnly"
    },
    {
        "key": "ctrl+backspace",
        "command": "deleteWordPartLeft",
        "when": "textInputFocus && !editorReadonly"
    },
    {   
        "key": "ctrl+delete",
        "command": "deleteWordPartRight",
        "when": "textInputFocus && !editorReadonly"
    },
    // search
    {
        "key": "ctrl+f",
        "command": "settings.action.search",
        "when": "inSettingsEditor"
    },
    {
        "key": "alt+right",
        "command": "editor.action.nextMatchFindAction",
        "when": "findWidgetVisible"
    },
    {
        "key": "alt+left",
        "command": "editor.action.previousMatchFindAction",
        "when": "findWidgetVisible"
    },
    // edit
    {
        "key": "shift+alt+up",
        "command": "editor.action.copyLinesDownAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "shift+alt+down",
        "command": "editor.action.copyLinesUpAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+d",
        "command": "editor.action.deleteLines",
        "when": "textInputFocus && !editorReadonly"
    },
    // md
    {
        "key": "ctrl+k ctrl+v",
        "command": "markdown.showPreviewToSide",
        "when": "!notebookEditorFocused && editorLangId == 'markdown'"
    },
    // LaTeX
    {
        "key": "ctrl+enter",
        "command": "-latex-workshop.build",
        "when": "editorTextFocus && !isMac"
    },
    {
        "key": "ctrl+k ctrl+v",
        "command": "-latex-workshop.view",
        "when": "config.latex-workshop.bind.altKeymap.enabled && editorLangId =~ /^latex$|^latex-expl3$|^rsweave$|^jlweave$|^pweave$/"
    },
    {
        "key": "ctrl+alt+c",
        "command": "-latex-workshop.clean",
        "when": "!config.latex-workshop.bind.altKeymap.enabled && editorLangId =~ /latex|rsweave|jlweave/"
    },
    // LaTeX L
    {
        "key": "ctrl+l alt+m",
        "command": "-latex-workshop.toggleMathPreviewPanel",
        "when": "config.latex-workshop.bind.altKeymap.enabled && editorLangId =~ /latex|latex-expl3|rsweave|jlweave/"
    },
    {
        "key": "ctrl+l ctrl+e",
        "command": "-latex-workshop.shortcut.emph",
        "when": "editorTextFocus && !editorReadonly && editorLangId =~ /^latex$|^latex-expl3$|^rsweave$|^jlweave$|^pweave$/"
    },
    {
        "key": "ctrl+l ctrl+b",
        "command": "-latex-workshop.shortcut.textbf",
        "when": "editorTextFocus && !editorReadonly && editorLangId =~ /latex|rsweave|jlweave/"
    },
    // LaTeX M
    {
        "key": "ctrl+m ctrl+b",
        "command": "-latex-workshop.shortcut.mathbb",
        "when": "editorTextFocus && !editorReadonly && editorLangId =~ /^latex$|^latex-expl3$|^rsweave$|^jlweave$|^pweave$/"
    },
    {
        "key": "ctrl+m ctrl+f",
        "command": "-latex-workshop.shortcut.mathbf",
        "when": "editorTextFocus && !editorReadonly && editorLangId =~ /^latex$|^latex-expl3$|^rsweave$|^jlweave$|^pweave$/"
    },
    {
        "key": "ctrl+m ctrl+b",
        "command": "latex-workshop.shortcut.mathbb",
        "when": "editorTextFocus && !editorReadonly && editorLangId =~ /latex|rsweave|jlweave/"
    },
]
