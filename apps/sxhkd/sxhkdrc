# apps
super + alt + {t, v, m, s, d, n, s}
	{kitty, code, microsoft-edge-stable, spotify, discord, nordpass, flameshot gui}

# rofi
super + @space
    rofi -show drun

super + shift + e
    rofi -show filebrowser

super + canc
    sh powermenu.sh

alt + f4
    sh powermenu.sh

super + w
	rofi -show window

# system
super + l
    xdg-screensaver lock